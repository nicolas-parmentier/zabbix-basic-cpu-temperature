                    <triggers>
                        <trigger>
                            <uuid>8242aa46431d4cc6898c96aa94baa362</uuid>
                            <expression>last(/Template basicTemp/basicTemp.cpu.max)&gt;{$TEMP.CPU.SOFT.MAX}</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>avg(/Template basicTemp/basicTemp.cpu.max,60s)&lt;{$TEMP.CPU.SOFT.MAX}-10</recovery_expression>
                            <name>CPU Temperature high</name>
                            <priority>AVERAGE</priority>
                            <dependencies>
                                <dependency>
                                    <name>CPU Temperature too high</name>
                                    <expression>last(/Template basicTemp/basicTemp.cpu.max)&gt;{$TEMP.CPU.SOFT.MAX}+5</expression>
